/* pages/wodeshebei/wodeshebei.wxss */

.zujiBox{margin: 30rpx;}
.zujiBox .box{
  border: 1rpx solid #f4cb99;
  border-radius: 15rpx;
  padding: 5rpx;
  margin-bottom: 20rpx;
}
.box_txt{
  background: #842b2e;
  width: 100%;
  border-radius: 15rpx;
  overflow: hidden;
  padding: 20rpx 10rpx;
  box-sizing: border-box;
}

.box_txt image{
  width: 130rpx;
  height: 130rpx;
  float: left;
}
.zujitxt{
  float: left;
  margin-left: 30rpx;
}
.zujitxt .tt1{
  display: block;
  font-size: 32rpx;
  margin-bottom: 20rpx;
  margin-top: 15rpx;
}
.zujitxt .tt2{
  display: block;
  font-size: 24rpx;
  color:#cfc3ab;
}












