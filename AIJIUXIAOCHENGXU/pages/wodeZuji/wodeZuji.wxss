/* pages/wodeZuji/wodeZuji.wxss */
.zujiBox{margin: 30rpx;}
.zujiBox .box{
  border: 1rpx solid #f4cb99;
  border-radius: 10rpx;
  padding: 5rpx;
  margin-bottom: 20rpx;
}
.box_txt{
  background: #842b2e;
  width: 100%;
  border-radius: 10rpx;
  overflow: hidden;
  padding:12rpx;
  box-sizing: border-box;
}
.box_txt image{
  width: 200rpx;
  height: 200rpx;
  float: left;
}
.zujitxt{
  float: left;
  margin:30rpx 0 0 30rpx;
}
.zujitxt .tt1{
  display: block;
  font-size: 32rpx;
  margin-bottom: 45rpx;
}
.zujitxt .tt2{
  display: block;
  font-size: 24rpx;
  color:#cfc3ab;
}












