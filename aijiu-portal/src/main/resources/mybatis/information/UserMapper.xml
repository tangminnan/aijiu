<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.aijiu.information.dao.UserDODao">

	<select id="get" resultType="com.aijiu.information.domain.UserDO">
		select `id`,`heard_url`,jianjie,`name`,`phone`,`height`,`weight`,`email`,`sex`,`birthday`,`nickname`,`username`,`password`,`open_id`,`last_login_time`,`register_time`,`delete_flag`,`province_code`,`province_name`,`city_code`,`city_name`,`area_code`,`area_name` from t_user where id = #{value}
