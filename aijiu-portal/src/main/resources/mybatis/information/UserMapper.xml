			`province_name`, 
			`city_code`, 
			`city_name`, 
			`area_code`, 
			`area_name`,
			jianjie
		)
		values
		(
			#{heardUrl}, 
			#{name}, 
			#{phone}, 
			#{height}, 
			#{weight}, 
			#{email}, 
			#{sex}, 
			#{birthday}, 
			#{nickname}, 
